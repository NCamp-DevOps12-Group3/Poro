<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<!-- mybatis에서 사용하는 설정 파일 -->
<configuration>
    <typeAliases>
        <typeAlias type="com.bit.devops12.poro.dto.ProfileDto" alias="profile"/>
        <typeAlias type="com.bit.devops12.poro.dto.PortfolioDto" alias="portfolio"/>
        <typeAlias type="com.bit.devops12.poro.dto.CommentDto" alias="comment"/>
        <typeAlias type="com.bit.devops12.poro.dto.FileDto" alias="file"/>
        <typeAlias type="com.bit.devops12.poro.dto.UserDto" alias="user"/>
        <typeAlias type="com.bit.devops12.poro.dto.CompanyDto" alias="company"/>
        <typeAlias type="com.bit.devops12.poro.dto.RecruitmentDto" alias="coperation"/>
        <typeAlias type="com.bit.devops12.poro.dto.UserFeedsMessageDto" alias="message"/>
        <typeAlias type="com.bit.devops12.poro.dto.HtmlDto" alias="html"/>
        <typeAlias type="com.bit.devops12.poro.dto.CssDto" alias="css"/>
        <typeAlias type="com.bit.devops12.poro.dto.JavascriptDto" alias="js"/>
    </typeAliases>

    <mappers>
        <mapper resource="mappings/user-mapper.xml"/>
        <mapper resource="mappings/mapper-userfeeds.xml"/>
        <mapper resource="mappings/portfolio-mapper.xml"/>
        <mapper resource="mappings/comment-mapper.xml"/>
        <mapper resource="mappings/company-mapper.xml"/>
        <mapper resource="mappings/mapper-message.xml"/>
        <mapper resource="mappings/html-mapper.xml"/>
        <mapper resource="mappings/css-mapper.xml"/>
        <mapper resource="mappings/js-mapper.xml"/>
    </mappers>

</configuration>